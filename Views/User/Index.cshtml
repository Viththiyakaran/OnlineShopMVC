@model IEnumerable<Users>
@{
    ViewData["Title"] = "Product";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<!-- End of Topbar -->
<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Products</h1>
        <a class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm" asp-controller="Product" asp-action="Create">
            @*<i class="fa fa-plus-circle fa-sm text-white-50"></i> Add Product*@
        </a>
    </div>
    <!-- Content Row -->
    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Customer Details</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>UID</th>
                            <th>FristName</th>
                            <th>LastName</th>
                            <th>Email</th>
                           
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>UID</th>
                            <th>FristName</th>
                            <th>LastName</th>
                            <th>Email</th>
                        </tr>
                    </tfoot>
                    <tbody>

                        @foreach (var Obj in Model)
                        {
                            <tr>
                                <td>@Obj.userid</td>
                                <td>@Obj.userFirstName</td>
                                <td>@Obj.userLastName</td>
                                <td>@Obj.userEmail</td>

                            </tr>

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>







</div>
<!-- /.container-fluid -->
